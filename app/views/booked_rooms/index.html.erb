<h1>Listing booked_rooms</h1>

<table>
  <tr>
    <th>Room</th>
    <th>From date</th>
    <th>To date</th>
    <th>Quantity</th>
    <th>Hotel</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @booked_rooms.each do |booked_room| %>
  <tr>
    <td><%= booked_room.room %></td>
    <td><%= booked_room.from_date %></td>
    <td><%= booked_room.to_date %></td>
    <td><%= booked_room.quantity %></td>
    <td><%= booked_room.hotel %></td>
    <td><%= link_to 'Show', booked_room %></td>
    <td><%= link_to 'Edit', edit_booked_room_path(booked_room) %></td>
    <td><%= link_to 'Destroy', booked_room, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Booked room', new_booked_room_path %>
